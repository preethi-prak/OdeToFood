@model IEnumerable<OdeToFood.Data.Models.Restaurant> 
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    @*<div>There are @Enumerable.Count(Model) Restaurants </div>*@
    <div>There are @Model.Count() Restaurants </div>

</div>
<ul>
    @foreach (var item in Model)
    {
        <li>@item.Name</li>
    }
</ul>

